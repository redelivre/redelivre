    <custom-types>
        <custom-type translate="1">event</custom-type>
        <custom-type translate="1">location</custom-type>
    </custom-types>    
    <taxonomies>
        <taxonomy translate="1">event-categories</taxonomy>
        <taxonomy translate="1">event-tags</taxonomy>
    </taxonomies>
