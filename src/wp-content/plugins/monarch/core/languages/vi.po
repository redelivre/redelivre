msgstr "Tải bản sao lưu trước khi nhập"

#: components/Portability.php:1178
msgid "Import %s"
msgstr "Nhập %s"

#: components/Portability.php:1179
msgid "Cancel Import"
msgstr "Hủy Nhập"

#: components/Portability.php:1281
msgid "Import & Export"
msgstr "Nhập & Xuất"

